<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rushikesh.coffee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/coffee"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/coffee"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".splash"
            android:label="Coffee"
            android:theme="@style/App_Bar_Style">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Coffee"
                   android:parentActivityName=".MainActivity"/>
        <activity android:name=".Tea"
                    android:parentActivityName=".MainActivity"/>
        <activity android:name=".milk_shake"
                    android:parentActivityName=".MainActivity"/>
        <activity android:name=".ice_cream"
                    android:parentActivityName=".MainActivity">

        </activity>
    </application>

</manifest>